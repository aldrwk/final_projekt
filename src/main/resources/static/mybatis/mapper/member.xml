<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.final_project.member.memberMapper">
    <insert id="insert" parameterType="memberDomain">
        insert into member
        values (null, #{email}, #{password}, #{name}, #{mobile_num}, #{profile}, #{create_date}, #{change_date})
    </insert>
    <select id="findById" resultType="memberDomain">
        select * from member where email = #{email}
    </select>
    <select id="findByMobile" resultType="memberDomain">
        select * from member where mobile_num = #{mobile_num}
    </select>


</mapper>