<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.final_project.bank.bankMapper">

    <select id="findById" resultType="accountDomain" resultMap="accountMap">
        select host_num, account_num, bank_code, account_owner from account;
    </select>
    <select id="findAll" resultType="accountDomain" resultMap="accountMap">
        select bank_code, bank_name from bank where bank_name = ${bankName};
    </select>



    <resultMap id="accountMap" type="accountDomain">
        <result column="account_id" property="accountId" />
        <result column="host_num" property="hostNum" />
        <result column="account_num" property="accountNum" />
        <result column="bank_code" property="bankCode" />
        <result column="account_owner" property="accountOwner" />
        <result column="change_date" property="changeDate" />
    </resultMap>
</mapper>